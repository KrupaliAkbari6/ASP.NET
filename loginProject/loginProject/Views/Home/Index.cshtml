@model List<loginProject.Models.Student>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <table class="table">
        <thead>
        <th>ID</th>
        <th>Username</th>
        <th>Password</th>
        <th>Name</th>
        <th>City</th>
        <th>Gender</th>
        </thead>

        @foreach (var item in Model)
        {
            <tbody>

                <tr>

                    <td>@item.Id</td>
                    <td>@item.Username</td>
                    <td>@item.Password</td>
                    <td>@item.Name</td>
                    <td>@item.City</td>
                    <td>@item.Gender</td>
                </tr>
            </tbody>
        }
        </table>
</div>

