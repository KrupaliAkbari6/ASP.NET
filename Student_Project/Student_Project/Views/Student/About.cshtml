@model Student_Project.Models.student
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</head>
<body>
    <div class="container">
        <nav class="navbar navbar-expand-lg bg-light">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="~/Student/Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="~/Student/About">About</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="~/Student/Contact">Contact</a>
                </li>
            </ul>
        </nav>
        <table class="table table-success table-bordered table-striped">
            <thead>
                <h2>Students Details</h2>
                <tr>
                    <th>Roll No</th>
                    <th>Name</th>
                    <th>City</th>
                    <th>Division</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@foreach (var item in (List<int>)ViewBag.id) {@item; <br / >}</td>
                    <td>@foreach (var item in (List<string>)ViewBag.name) {@item; <br />}</td>
                    <td>@foreach (var item in (List<string>)ViewBag.city) {@item; <br />}</td>
                    <td>@foreach (var item in (List<string>)ViewBag.division) {@item; <br />}</td>
                </tr>

            </tbody>
        </table>
    </div>
</body>
</html>
